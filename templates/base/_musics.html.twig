<div class="text-center">

    <div class="musics_slide">
        <i class="fas fa-chevron-left"></i>
    </div>

    <h3>Musiques</h3>  
    
    <div class="text-center mb-3"><a href="{{ path('all_musics') }}">Voir tout</a></div>

    <hr>

    {# Nouveaut√©s? #}

    {# Listing_musiques #}
    {% for music in musics %}        
        <div class="musics"
            data-musics-audio="{{ music|music }}"
            data-musics-image="{{ music|image }}"
            data-musics-artist="{{ music.artist }}"
            data-musics-title="{{ music.title }}"
        >
            <div class="musics_widgets_img">
                <img src="{{ asset(music|image) }}" alt="image de {{music.title}}">
            </div>
            <div class="musics_widgets_info">
                <p class="fs-6">{{ music.title }}</p>
                <p>{{ music.artist }}</p>
            </div>       
        </div>   
        <hr>
          
    {% endfor %}
    
</div>
